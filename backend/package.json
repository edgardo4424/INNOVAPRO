{
    "name": "backend",
    "version": "1.0.0",
    "lockfileVersion": 3,
    "main": "src/index.js",
    "scripts": {

        "dev": "nodemon src/index.js",
        "start": "node src/index.js",
        "test": "jest",
        
        "test:quinta": "jest src/modules/quintaCategoria/tests --verbose --runInBand",
        "test:quinta:watch": "jest src/modules/quintaCategoria/tests --watch",
        "test:quinta:cov": "jest src/modules/quintaCategoria/tests --coverage",


        
        "migrate": "sequelize-cli db:migrate --env development",
        "migrate:status": "sequelize-cli db:migrate:status --env development",
        "migrate:undo": "sequelize-cli db:migrate:undo --env development",
        "migrate:undo:all": "sequelize-cli db:migrate:undo:all --env development",
        "migrate:redo": "npm run migrate:undo && npm run migrate",


        "crear:migracion": "sequelize-cli migration:create --env development --name",
        
        "crear:modelo": "sequelize-cli model:generate --env development",
        "crear:seed": "sequelize-cli seed:generate --env development --name",

        
        "seed": "sequelize-cli db:seed:all --env development",
        "seed:undo": "sequelize-cli db:seed:undo:all --env development",
        "seed:one": "sequelize-cli db:seed --env development",
        "seed:undo:one": "sequelize-cli db:seed:undo --env development",

    
        "db:create": "sequelize-cli db:create --env development",
        "db:drop": "sequelize-cli db:drop --env development",
        "db:check": "node scripts/db-check.js",

        
        "migrate:prod": "sequelize-cli db:migrate --env production",
        "migrate:status:prod": "sequelize-cli db:migrate:status --env production",
        "migrate:undo:prod": "sequelize-cli db:migrate:undo --env production",
        "migrate:undo:all:prod": "sequelize-cli db:migrate:undo:all --env production",
        "seed:prod": "sequelize-cli db:seed:all --env production",
        "seed:undo:prod": "sequelize-cli db:seed:undo:all --env production"

    },
    "dependencies": {
        "axios": "^1.8.4",
        "bcrypt": "^5.1.1",
        "bcryptjs": "^3.0.2",
        "compression": "^1.8.0",
        "cors": "^2.8.5",
        "crypto-js": "^4.2.0",
        "date-fns": "^4.1.0",
        "dotenv": "^16.4.7",
        "express": "^4.18.2",
        "express-validator": "^7.2.1",
        "glob": "^11.0.1",
        "graceful-fs": "^4.2.11",
        "har-validator": "^5.1.5",
        "helmet": "^8.0.0",
        "iconv-lite": "^0.6.3",
        "jsonwebtoken": "^9.0.2",
        "lz-string": "^1.5.0",
        "minimatch": "^10.0.1",
        "morgan": "^1.10.1",
        "moment": "^2.30.1",
        "moment-timezone": "^0.6.0",
        "mysql2": "^3.6.0",
        "node-cron": "^3.0.3",
        "node-fetch": "^2.7.0",
        "node-telegram-bot-api": "^0.66.0",
        "normalize-package-data": "^7.0.0",
        "playwright": "^1.54.2",
        "rimraf": "^4.4.1",
        "semver": "^7.7.1",
        "sequelize": "^6.37.5",
        "socket.io": "^4.8.1",
        "socket.io-client": "^4.8.1",
        "unzipper": "^0.12.3",
        "uuid": "^11.1.0",
        "ws": "^8.18.1"
    },
    "devDependencies": {
        "jest": "^29.7.0",
        "nodemon": "^3.1.9",
        "sequelize-cli": "^6.6.2",
        "supertest": "^7.1.0"
    },
    "author": "",
    "license": "ISC"
}
